#include <stdio.h>

float pi()
{
    
    return 3.142;
}

int sum(int a,int b)
{
    int c;
    c=a+b;
    return c;
}

int main()
{
    int x=10,y=20,z;
    printf("The value of pi is %f \n",pi());
    z=sum(x,y);
    printf("the sum of %d and %d is %d",x,y,z);
 
    return 0;
}

//O/p: The value of pi is 3.142000 
//     the sum of 10 and 20 is 30
