#include <stdio.h>

float div(float a,float b)
{
    float c=a/b;
    return c;
}

// In case of type casting
/* float div(int a,int b)
{
    float c=(float)a/(float)b;
    return c;
} */

int sum(int a,int b)
{
    int c;
    c=a+b;
    return c;
}

int main()
{
    int x=10,y=20,z;
    float d=div(22,7);
    printf("The value of pi is %f \n",d);
    z=sum(x,y);
    printf("the sum of %d and %d is %d",x,y,z);
 
    return 0;
}

//O/p: The value of pi is 3.142857
//     the sum of 10 and 20 is 30
